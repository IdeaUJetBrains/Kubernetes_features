{
  "kind": "ReplicationController",
  "apiVersion": "v1",
  "metadata": {
    "name": "guestbook",
    "ownerReferences": []
  },
  "spec": {
    "replicas": "a3",
    "selector": {
      "app": "guestbook"
    },
    "template": {
      "metadata": {
        "labels": {
          "app": "guestbook"
        }
      },
      "spec": {
        "containers": [
          {
            "name": "guestbook",
            "image": "gcr.io/google_containers/guestbook:v3",
            "ports": [
              {
                "name": "http-server",
                "containerPort": 3000
                "hostIP1": "",
                "hostPort": "444",
                "protocol": ""
              }
            ]
          }
        ]
      }
    }
  }
}
